{
  "name": "vtk-wasm-benchmarks",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-wasm": "npm run build-wasm-with-developer-vtk:debug",
    "build-wasm:release": "npx itk-wasm -i kitware/vtk-wasm -b out -s . build -- Release -DDEBUGINFO=NONE -DOPTIMIZE=BEST",
    "build-wasm:debug": "npx itk-wasm -i kitware/vtk-wasm -b out -s . build -- Debug -DDEBUGINFO=DEBUG_NATIVE -DOPTIMIZE=NO_OPTIMIZATION",
    "build-wasm-with-developer-vtk:release": "emcmake cmake -GNinja -S . -B out -DVTK_DIR=/home/local/KHQ/jaswant.panchumarti/Documents/code/ansys/vtk/install/lib/cmake/vtk-9.3 -DDEBUGINFO=NONE -DOPTIMIZE=BEST && cmake --build out",
    "build-wasm-with-developer-vtk:debug": "emcmake cmake -GNinja -S . -B out -DVTK_DIR=/home/local/KHQ/jaswant.panchumarti/Documents/code/ansys/vtk/install/lib/cmake/vtk-9.3 -DDEBUGINFO=DEBUG_NATIVE -DOPTIMIZE=NO_OPTIMIZATION && cmake --build out",
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite --cors --base /",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "core-js": "^3.29.0",
    "roboto-fontface": "*",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@types/emscripten": "^1.39.9",
    "@types/node": "^18.18.6",
    "@types/stats.js": "^0.17.2",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@webgpu/types": "^0.1.38",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "lil-gui": "^0.18.2",
    "sass": "^1.60.0",
    "stats.js": "^0.17.0",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
